			value = 
			{
				_class = "CCommentAnimNode"
				m_sName = "Additive PelvisOnly compositing for weapons"
				m_vecPosition = [ 96.0, -4912.0 ]
				m_nNodeID = 
				{
					m_id = 382772088
				}
				m_networkMode = "ClientSimulate"
				m_sNote = ""
				m_commentText = "The \"Model Space blended pose\" nodes are compositing the pose on top of the movement anims using \"world space blending\" (or model space, as it\'s known now). The idle input of each weapon\'s State Machine is a 1D Blend which itself looks at the Duck parameter to know whether it needs to feed the standing pose or ducking pose.\n\nThis causes a problem, however: because the other anims are only created \"against\" the standing pose, we can\'t compute a proper \"PelvisOnly\" additive unless we get the crouched pose out of the way.\n\nCurrently, this is done by duplicating each weapon\'s state machine, and changing the Idle input of that one, so it only ever gets the standing pose. This isn\'t particularly elegant, and means copy/pasting if other inputs are ever to be added... but it works well enough for now!"
				m_size = [ 656.0, 496.0 ]
				m_color = [ 65, 105, 225, 255 ]
			}
